package main

import (
	"fmt"
	"math"
)

func pangkat(base, pangkat int) int {
	if pangkat ==0 {
	return 1
	} else {
		return base * pangkat(base,pangkat-1)
	}
}

func main() {
	fmt.Println(pangkat(2, 3))
	fmt.Println(pangkat(5, 3))
	fmt.Println(pangkat(10, 2))
	fmt.Println(pangkat(2, 5))
	fmt.Println(pangkat(7, 3))
}
